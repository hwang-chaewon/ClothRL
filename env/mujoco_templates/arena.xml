        <camera fovy="{{train_camera_fovy}}" mode="targetbody" target="lookatbody" name="up" pos="0.5 -0.7  1.1"/>
        <camera fovy="{{train_camera_fovy}}" mode="targetbody" target="lookatbody" name="side" pos="-0.4 -0.7  0.65"/> 
